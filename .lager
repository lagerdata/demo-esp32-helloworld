[LAGER]

[DEVENV]
image = lagerdata/devenv-esp32:version-1.0
mount_dir = /github/workspace
shell = /bin/bash
cmd.build = idf.py build
cmd.clean = rm -rf build
cmd.flash = cp build/bootloader/bootloader.bin lagerpy/ && cp build/partition_table/partition-table.bin lagerpy/ && cp build/hello-world.bin lagerpy/ && lager python --gateway 45 lagerpy
cmd.menuconfig = idf.py menuconfig

